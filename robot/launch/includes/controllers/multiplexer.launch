
<launch>
  <arg name="machine" default="robot"/>

  <node name="multiplexer" pkg="twist_mux" type="twist_mux" machine="$(arg machine)">
    <remap from="cmd_vel_out" to="output/cmd_vel"/>

    <rosparam command="load" file="$(find robot)/param/twist_mux_locks.yaml"/>
    <rosparam command="load" file="$(find robot)/param/twist_mux_topics.yaml"/>
  </node>

  <node name="twist_marker" pkg="twist_marker" type="twist_marker" machine="$(arg machine)">
    <remap from="twist" to="output/cmd_vel"/>
  </node>
</launch>